<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="QMS Automation Suite">
  <listeners>
	<listener class-name="com.yeasin.appium_qms.utilities.Listeners"/>
  </listeners>
  
  <test thread-count="5" name="QMS Online Basic Test">
    <classes>
      <class name="com.yeasin.appium_qms.QmsOnlineNormal"/>
    </classes>
  </test> <!-- QMS Appium Test -->
  
  <test thread-count="5" name="QMS Online Repair Test">
    <classes>
      <class name="com.yeasin.appium_qms.QmsOnlineRepair"/>
    </classes>
  </test> <!-- QMS Appium Test -->
  
  <test thread-count="5" name="QMS Offline Basic Test">
    <classes>
      <class name="com.yeasin.appium_qms.QmsOfflineNormal"/>
    </classes>
  </test> <!-- QMS Appium Test -->
  
  <test thread-count="5" name="QMS Offline Repair Test">
    <classes>
      <class name="com.yeasin.appium_qms.QmsOfflineRepair"/>
    </classes>
  </test> <!-- QMS Appium Test -->
  
</suite> <!-- QMS Automation Suite -->
